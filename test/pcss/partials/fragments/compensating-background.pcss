.partial[data-partial="fragments/compensating-background"] {
  /* DOCS START */
  [data-pkg="agency-pkg-documentation/doc-template/preview"] & {
    position: relative;
    &:before {
      content: "";
      display: block;
      padding-top: calc((9 / 16) * 100)%;
    }
    overflow: hidden;
  }
  perspective: 1000px;
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  /* DOCS END */
  [data-hook="content"] {
    transform-style: preserve-3d;
    display: block;
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    transform-origin: center center;
    picture {
      position: static;
      img {}
    }
  }
  picture {
    position: static;
    img {
      display: block;
      position: absolute;
      width: auto;
      height: 120%;
      @media (--screen-xs) {
        width: 120%;
        height: auto;
      }
      @media (--screen-sm) {
        width: auto;
        height: 120%;
      }
      @media (--screen-md) {
        width: 120%;
        height: auto;
      }
      @media (--screen-md) {}
      top: -170%;
      left: -170%;
      bottom: -170%;
      right: -170%;
    }
  }
}
